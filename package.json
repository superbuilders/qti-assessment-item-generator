{
  "name": "@superbuilders/qti-assessment-item-generator-app",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "dev": "bun --watch src/index.ts",
    "dev:inngest": "inngest-cli dev --queue-workers 4000 --poll-interval 2",
    "lint": "biome lint && bun run --cwd packages/lib lint",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:drop": "bun run src/db/scripts/drop.ts",
    "test": "bun test"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0-beta.6",
    "@types/bun": "^1.3.1",
    "@types/he": "^1.2.3",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "inngest-cli": "^1.13.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.922.0",
    "@superbuilders/errors": "^3.0.2",
    "@superbuilders/slog": "4.0.0",
    "@t3-oss/env-core": "^0.13.8",
    "drizzle-orm": "^0.44.7",
    "fast-xml-parser": "^5.3.0",
    "he": "^1.2.0",
    "hono": "^4.10.4",
    "inngest": "^3.44.4",
    "linkedom": "^0.18.12",
    "openai": "^6.7.0",
    "zod": "^4.1.12"
  }
}
