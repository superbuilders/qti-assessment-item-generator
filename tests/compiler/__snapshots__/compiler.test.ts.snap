    </qti-response-declaration>
    <qti-response-declaration identifier="dropdown_2" cardinality="single" base-type="identifier">
        <qti-correct-response>
            <qti-value>nonlinear</qti-value>
        </qti-correct-response>
    </qti-response-declaration>
    <qti-response-declaration identifier="dropdown_5" cardinality="single" base-type="identifier">
        <qti-correct-response>
            <qti-value>one_fourth</qti-value>
