                <qti-correct identifier="RESPONSE_TEXT_ENTRY"/>
            </qti-match>
            <qti-set-outcome-value identifier="FEEDBACK__GLOBAL">
                <qti-base-value base-type="identifier">CORRECT</qti-base-value>
            </qti-set-outcome-value>
        </qti-response-if>
        <qti-response-else>
